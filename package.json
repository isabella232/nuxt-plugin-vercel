{
  "name": "nuxt-plugin-vercel",
  "version": "0.0.0-development",
  "description": "Track Core Web Vitals in Nuxt projects with Vercel Analytics.",
  "scripts": {
    "test": "exit 0",
    "prepublishOnly": "yarn test",
    "semantic-release": "semantic-release",
    "commit": "git-cz"
  },
  "dependencies": {
    "web-vitals": "0.2.4"
  },
  "devDependencies": {
    "cz-conventional-changelog": "3.2.0",
    "husky": "4.2.5",
    "prettier": "2.1.2",
    "pretty-quick": "2.0.1",
    "semantic-release": "17.1.1"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "husky": {
    "hooks": {
      "pre-commit": "pretty-quick --staged"
    }
  },
  "keywords": [
    "nuxt",
    "web vitals",
    "vercel",
    "cls",
    "fcp",
    "fid",
    "lcp",
    "ttfb"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/vercel/nuxt-plugin-vercel.git"
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/vercel/nuxt-plugin-vercel/issues"
  },
  "homepage": "https://github.com/vercel/nuxt-plugin-vercel#readme",
  "main": "module.js",
  "files": [
    "module.js",
    "plugin.js"
  ],
  "prettier": {
    "arrowParens": "avoid",
    "singleQuote": true,
    "tabWidth": 2,
    "semi": false,
    "trailingComma": "none"
  }
}
